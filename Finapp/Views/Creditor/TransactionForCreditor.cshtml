@model Finapp.ViewModels.CreditorTransactionViewModel

@{
    ViewBag.Title = "TransactionForCreditor";
}

<div class="table-responsive">
    <table class="table table-bordered table-hover">
        <thead>
            <tr>
                <th>Creditor Username</th>
                <th>Deptor Account</th>
                <th>Date</th>
                <th>Amount</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var Transaction in Model.Transactions)
            {
                <tr>
                    <td>@Model.Creditor.username</td>
                    <td>@Transaction.Debtor_Account_Id</td>
                    <td>@Transaction.Date_Of_Transaction</td>
                    <td>@Transaction.Ammount</td>
                </tr>
            }
        </tbody>
    </table>
</div>